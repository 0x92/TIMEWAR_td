{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ArtifactsFile",
  "type": "object",
  "required": ["artifacts"],
  "additionalProperties": false,
  "properties": {
    "artifacts": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["id", "name", "effect", "value"],
        "additionalProperties": false,
        "properties": {
          "id": { "type": "string" },
          "name": { "type": "string" },
          "effect": { "type": "string" },
          "value": { "type": "number" }
        }
      }
    }
  }
}
